@charset "UTF-8";
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.index_swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
  width: 100%;
}

.index_cate .nav {
  padding: 12rpx;
  flex: 1;
}

.index_cate .nav image {
  width: 100%;
}

.index_floor .floor_title image {
  width: 100%;
}

.index_floor .floor_list {
  overflow: hidden;
}

.index_floor .floor_list navigator {
  float: left;
  width: 33.33%;
  /* 后四个超链接 */
  /* 2 3 两个超链接 */
}

.index_floor .floor_list navigator:nth-last-child(-n+4) {
  /* 原图的宽高 232 *386 */
  height: 27.72711vw;
  border-left: 10rpx solid #fff;
}

.index_floor .floor_list navigator:nth-child(2), .index_floor .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}

.index_floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}
